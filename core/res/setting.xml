<setting>
	<data>
		<!-- Requirement -->
		<var name='water-density'>requirement/environment/water-density</var>
		<var name='rotate-efficiency'>requirement/technical/rotate-efficiency</var>
		<var name='drivetrain-efficiency'>requirement/technical/drivetrain-efficiency</var>
		<var name='water-coefficient'>requirement/technical/water-coefficient</var>
		<var name='thrust-deduction-coefficient'>requirement/technical/thrust-deduction-coefficient</var>
		<!-- Geometry: blade -->
		<var name='blade-count'>geometry/blade-count</var>
		<var name='diameter'>geometry/diameter</var>
		<var name='pitch'>geometry/pitch</var>
		<var name='pitch-ratio'>geometry/pitch-ratio</var>
		<var name='add-pitch-angle'>geometry/add-pitch-angle</var>
		<var name='ref-chord'>geometry/ref-chord</var>
		<!-- Geometry: area -->
		<var name='area-disc'>geometry/area/disc</var>
		<var name='area-projected'>geometry/area/projected</var>
		<var name='area-expanded'>geometry/area/expanded</var>
		<var name='projected-ratio'>geometry/area/projected-ratio</var>
		<var name='expanded-ratio'>geometry/area/expanded-ratio</var>
		<!-- Geometry: hub -->
		<var name='hub-diameter-ratio'>geometry/hub/diameter-ratio</var>
		<var name='hub-length'>geometry/hub/length</var>
		<var name='hub-cone-angle'>geometry/hub/cone-angle</var>
		<var name='shaft-diameter-ratio'>geometry/shaft/diameter-ratio</var>
		<!-- Performance: design point -->
		<var name='design-power'>performance/design/power</var>
		<var name='design-rotate-frequency'>performance/design/rotate-frequency</var>
		<var name='design-ship-speed'>performance/design/ship-speed</var>
		<var name='design-advance-speed'>performance/design/advance-speed</var>
		<var name='design-advance-coefficient'>performance/design/advance-coefficient</var>
		<var name='design-thrust-coefficient'>performance/design/KT</var>
		<var name='design-torque-coefficient'>performance/design/KQ</var>
		<var name='design-efficiency'>performance/design/efficiency</var>
		<var name='design-thrust'>performance/design/thrust</var>
		<var name='design-thrust-installed'>performance/design/thrust-installed</var>
		<var name='design-torque'>performance/design/torque</var>
		<var name='design-atlas-bp'>performance/design/atlas-bp</var>
		<!-- Performance: offdesign point -->
		<var name='offdesign-power'>performance/offdesign/power</var>
		<var name='offdesign-rotate-frequency'>performance/offdesign/rotate-frequency</var>
		<var name='offdesign-ship-speed'>performance/offdesign/ship-speed</var>
		<var name='offdesign-advance-speed'>performance/offdesign/advance-speed</var>
		<var name='offdesign-advance-coefficient'>performance/offdesign/advance-coefficient</var>
		<var name='offdesign-thrust-coefficient'>performance/offdesign/KT</var>
		<var name='offdesign-torque-coefficient'>performance/offdesign/KQ</var>
		<var name='offdesign-efficiency'>performance/offdesign/efficiency</var>
		<var name='offdesign-thrust'>performance/offdesign/thrust</var>
		<var name='offdesign-thrust-installed'>performance/offdesign/thrust-installed</var>
		<var name='offdesign-torque'>performance/offdesign/torque</var>
		<var name='offdesign-atlas-bp'>performance/offdesign/atlas-bp</var>
	</data>
	<methods>
		<method title="design_atlas_best" comment="Design a propeller with best efficiency by atlas method">
			<input>design-power, design-ship-speed, design-rotate-frequency, water-coefficient, thrust-deduction-coefficient, rotate-efficiency, drivetrain-efficiency, water-density</input>
			<output>diameter, pitch, pitch-ratio, design-advance-speed, design-advance-coefficient, design-efficiency, design-thrust-coefficient, design-torque-coefficient, design-torque, design-thrust, design-thrust-installed, design-atlas-bp</output>
		</method>
		<method title="design_atlas_fixD" comment="Calculate performance by atlas method">
			<input>design-power, design-ship-speed, design-rotate-frequency, diameter, water-coefficient, thrust-deduction-coefficient, rotate-efficiency, drivetrain-efficiency, water-density</input>
			<output>pitch, pitch-ratio, design-advance-speed, design-advance-coefficient, design-efficiency, design-thrust-coefficient, design-torque-coefficient, design-torque, design-thrust, design-thrust-installed, design-atlas-bp</output>
		</method>
		<method title="atlas_offdesign" comment="Calculate offdesign performance by atlas method">
			<input>diameter, pitch-ratio, offdesign-ship-speed, offdesign-rotate-frequency, water-density, water-coefficient, thrust-deduction-coefficient, rotate-efficiency, drivetrain-efficiency</input>
			<output>offdesign-power, offdesign-advance-speed, offdesign-advance-coefficient, offdesign-efficiency, offdesign-thrust-coefficient, offdesign-torque-coefficient, offdesign-torque, offdesign-thrust, offdesign-thrust-installed, offdesign-atlas-bp</output>
		</method>
		<method title="blade_pitch" comment="Calculating pitch by pitch ratio">
			<input>diameter, pitch-ratio</input>
			<output>pitch</output>
		</method>
		<method title="blade_profile" comment="Generating geometry body of profile">
			<input>blade-count, diameter, pitch, add-pitch-angle, ref-chord</input>
		</method>
		<method title="blade_chord" comment="Calculating blade ref chord by Ae/A0">
			<input>diameter, expanded-ratio</input>
			<output>ref-chord</output>
		</method>
		<method title="blade_area" comment="Calculating characteristic areas">
			<input>diameter, ref-chord, blade-count</input>
			<output>area-disc, area-projected, area-expanded, projected-ratio</output>
		</method>
		<method title="show_outline" comment="Display 2D drawing of propeller"></method>
		<method title="show_geometry_feature" comment="Display profile (geometry) features of propeller">
			<input>hub-diameter-ratio, hub-length, hub-cone-angle, shaft-diameter-ratio</input>
		</method>
		<method title="show_hydrodynamic_feature" comment="Display hydrodynamic features of propeller">
			<input>diameter, ref-chord, pitch, add-pitch-angle, offdesign-advance-speed, offdesign-rotate-frequency</input>
		</method>
		<method title="generate_grip_code" comment="Generating grip for NX"></method>
		<method title="export_to_stl" comment="Export blade to stl file"></method>
	</methods>
</setting>

